{
	"name": "satellite-visualization",
	"version": "0.1.0",
	"type": "module",
	"description": "3D visualization of a satellite orbiting Earth",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint": "pnpm lint-biome && pnpm lint-md",
		"lint-biome": "biome check --write --no-errors-on-unmatched --max-diagnostics=none",
		"lint-md": "dprint fmt",
		"check-types": "tsc --noEmit",
		"prepare": "lefthook install",
		"opensrc": "opensrc"
	},
	"keywords": [
		"satellite",
		"3d",
		"visualization",
		"threejs",
		"orbit"
	],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.26.1",
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@tanstack/react-router-devtools": "^1.150.0",
		"@tanstack/router-plugin": "^1.150.0",
		"@types/react": "^19.2.8",
		"@types/react-dom": "^19.2.3",
		"@types/three": "^0.182.0",
		"@vitejs/plugin-react": "^5.1.2",
		"dprint": "^0.51.1",
		"lefthook": "^2.0.15",
		"opensrc": "^0.6.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.1"
	},
	"dependencies": {
		"@base-ui/react": "^1.0.0",
		"@effect-atom/atom": "^0.4.13",
		"@effect-atom/atom-react": "^0.4.5",
		"@react-three/drei": "^10.7.7",
		"@react-three/fiber": "^9.5.0",
		"@tanstack/react-query": "^5.90.17",
		"@tanstack/react-router": "^1.150.0",
		"effect": "^3.19.14",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"three": "^0.182.0"
	}
}
